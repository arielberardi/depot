<%= turbo_stream_from 'products' %>
<div class="w-full">
  <%= render 'notice' %>
  <h1 class="font-bold text-xl mb-6 pb-2 border-b-2"><%= t('store.index.title') %></h1>
  <ul>
    <% cache @product do %>
      <% @products.each do |product| %>
        <% cache product do %>
          <%= render partial: 'product', object: product %>
        <% end %>
      <% end %>
    <% end %>
  </ul>
</div>
